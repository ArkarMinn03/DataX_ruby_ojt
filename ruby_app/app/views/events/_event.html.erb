<div id="<%= dom_id event %>">
  <p>
    <strong>Title : </strong>
    <%= event.title %>
  </p>
  <p>
    <strong>Description : </strong>
    <%= event.description %>
  </p>
  <p>
    <strong>Start Time : </strong>
    <%= event.formated_start_time %>
  </p>
  <p>
    <strong>End Time : </strong>
    <%= event.formated_end_time %>
  </p>
</div>